class Solution:
    def missingNumber(self, nums: List[int]) -> int:
        nums.sort()
        k=len(nums)
        for i in range(len(nums)):
            if nums[i]!=i:
                k=i
                break
        return k

#soln 5
class Solution:
    def missingNumber(self, nums: List[int]) -> int:
        nums.sort()
        for i in range(len(nums)):
            if nums[i]!=i:
                return i
        return len(nums)

